<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="EditTextStandard" parent="@android:style/Widget.EditText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">2dip</item>
        <item name="android:layout_marginRight">2dip</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:layout_width">fill_parent</item>
    </style>

    <style name="Entradas" parent="@android:style/Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0dip</item>
        <item name="android:layout_marginRight">5dip</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    
    <style name="Prato" parent="@android:style/Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0dip</item>
        <item name="android:layout_marginRight">5dip</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:textColor">#24B711</item>
    </style>
    
    <style	name="Topo" parent="@android:style/Widget.TextView" >
    	<item name="android:layout_height">wrap_content</item>
    	<item name="android:textStyle">bold</item>
    	<item name="android:gravity">center</item>
    	<item name="android:padding">7dip</item>
    </style>
</resources>